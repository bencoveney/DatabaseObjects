<h2>Items</h2>

<# foreach(Item item in model.Items.Values) { #>

	<h3><a name="<#= item.Name #>"></a><#= item.Name #></h3>
  
  <p><#= GetDescription(item) #></p>
  
	<h4>Attributes</h4>

<#	foreach(IAttribute attribute in item.Attributes.Values)
	{
		WriteAttribute(attribute);
	}
#>

	<hr />

<# } #>